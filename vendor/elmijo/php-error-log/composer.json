{
    "name": "elmijo/php-error-log",
    "type": "library",
    "description": "Una pequeña libreria para manejar los logs de tu aplicación de forma simple.. rapida.. y directa!!",
    "keywords": ["log", "logging","php","error","error_log"],
    "homepage": "https://github.com/ElMijo/php-error-log",
    "version": "2.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerry Anselmi",
            "email": "jerry.anselimi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.0",
        "elmijo/php-cliente-addr": "2.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PHPTools\\PHPErrorLog\\":"src/PHPErrorLog/"
        }
    }
}
